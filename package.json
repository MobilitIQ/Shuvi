{
  "name": "shuvi",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "tsc": "tsc",
    "dev": "ts-node-dev --respawn --transpileOnly ./app/app.ts",
    "clean": "rm -rf build",
    "rebuild:dev": "yarn run clean && yarn run dev",
    "rebuild:prod": "yarn run clean && yarn run prod",
    "build:prod": "tsc ./app/app.ts",
    "run:prod": "node ./build/app.js",
    "prod": "yarn test && yarn build:prod && yarn run:prod",
    "test": "ts-node-dev --transpileOnly ./test/test.ts"
  },
  "dependencies": {
    "@types/express": "^4.17.2",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "eslint": "^6.8.0",
    "express": "^4.17.1",
    "typescript": "^3.7.5"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2"
  },
  "devDependencies": {
    "ts-node-dev": "^1.0.0-pre.44"
  }
}
